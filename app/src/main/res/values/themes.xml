<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.ApplicationLanch" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/purple_500</item>
        <item name="colorPrimaryVariant">@color/purple_700</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/darkBrown</item>
        <item name="colorSecondaryVariant">@color/darkBrown</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">@color/darkBrown</item>
        <!-- Customize your theme here. -->
    </style>
    
    <style name="Spinner" parent="Widget.AppCompat.Light.DropDownItem.Spinner">
        
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">20sp</item>
        <item name="background">@color/darkBrown</item>
        <item name="backgroundTint">@color/darkBrown</item>

    </style>
</resources>